user  nginx;

events {
  worker_connections   1000;
}

http {
  server {
    listen 80;
    listen [::]:80;
    server_name <custom-domain>;
    server_tokens off;

    location /.well-known/acme-challenge/ {
      root /var/www/certbot;
    }

    location / {
      proxy_pass http://openwebui:3000;
    }
  }
}

